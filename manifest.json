{
	"name": "NOTE RINSVENT",
	"background": {
		"scripts": [ "assets/js/jquery.js","assets/js/background.js", "oauth/chrome_ex_oauthsimple.js", "oauth/chrome_ex_oauth.js", "oauth/onload.js", "oauth/init.js" ]
	},
	"browser_action": {
		"default_icon": {
			"16": "icons/16.png",
			"32": "icons/32.png"
		},
		"default_popup": "popup.html",
		"default_title": "NOTE RINSVENT"
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"chromeos": "Alt+R",
				"linux": "Alt+R",
				"mac": "Alt+R",
				"windows": "Alt+R"
			}
		},
		"_execute_page_action": {
			"suggested_key": {
				"default": "Alt+R",
				"mac": "Alt+R",
				"windows": "Alt+R"
			}
		}
	},
	"content_scripts": [ {
		"all_frames": true,
		"js": [ "assets/js/jquery.js", "assets/js/script.js" ],
		"matches": [ "<all_urls>" ]
	} ],
	"default_locale": "ru",
	"description": "note.rinsvent.ru request plugin",
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png"
	},
	"manifest_version": 2,
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"contextMenus",
		"https://www.google.com/accounts/OAuthGetRequestToken",
		"https://www.google.com/accounts/OAuthAuthorizeToken",
		"https://www.google.com/accounts/OAuthGetAccessToken"
	],
	"short_name": "Клиент сайта note.rinsvent.ru для простого поиска/добавления информации",
	"version": "1.0.0"
}
